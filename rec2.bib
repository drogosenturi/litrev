@article{ WOS:000354644800011,
Author = {Miller, Kelly K. and Blaszczynski, Veronika N. and Weston, Michael A.},
Title = {Feeding wild birds in gardens: A test of water versus food},
Journal = {ECOLOGICAL MANAGEMENT \& RESTORATION},
Year = {2015},
Volume = {16},
Number = {2},
Pages = {156-158},
Month = {MAY},
Abstract = {Bird feeding in residential gardens is an increasingly popular
   human-wildlife interaction. In Australia, the practice is discouraged by
   most government and nongovernment wildlife conservation agencies,
   although advice varies and the most common recommendation is to provide
   water and habitat for birds rather than supplementary food. This study
   compares bird abundance and diversity when residents in a Melbourne
   municipality provide water for birds versus food. Bird abundance was
   greater when food was provided compared with water, but avian
   assemblages did not differ.},
