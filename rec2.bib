@article{ WOS:000260849800013,
Author = {Fetridge, Evelyn D. and Ascher, John S. and Langellotto, Gail A.},
Title = {The Bee Fauna of Residential Gardens in a Suburb of New York City
   (Hymenoptera: Apoidea)},
