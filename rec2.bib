@article{ WOS:000638076700001,
Author = {Steven, Rochelle and Van Helden, Bronte E. and Tulloch, Ayesha I. and
   Barnes, Megan and Close, Paul G. and Fuller, Richard A.},
Title = {Exploring the ability of urban householders to correctly identify
   nocturnal mammals},
Journal = {URBAN ECOSYSTEMS},
