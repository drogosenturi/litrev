	file = {Full Text PDF:/home/drogo/Zotero/storage/PBUAHBHG/Lerman et al. - 2023 - Humanity for Habitat Residential Yards as an Oppo.pdf:application/pdf;Snapshot:/home/drogo/Zotero/storage/GJQ4GCC6/7289288.html:text/html},
}

