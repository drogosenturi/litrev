	doi = {10.1089/eco.2021.0064},
	abstract = {A strong connection with nature promotes behaviors that help conserve the natural world. However, it is likely that this relationship is reciprocal, with proconservation behaviors positively impacting nature connectedness by increasing sensory contact with nature. Proconservation behaviors vary in terms of how much visible biodiversity, and therefore contact with nature, they produce. It is likely that conservation behaviors that support higher visible biodiversity will result in more sensory contact with nature and, therefore, greater levels of nature connectedness. This research explores the relationship between garden-focused pronature conservation behavior, noticing nature and nature connectedness using data from Natural England's People and Nature Survey in the United Kingdom (n = 4206), a large national survey that includes items to measure noticing nature, nature connectedness, and pronature conservation behaviors. Results are consistent with the hypothesis that undertaking garden-based pronature conservation behaviors that enhance visible biodiversity leads to an increase in noticing nature, which, in turn, leads to an increase in nature connectedness. These results point to a relatively simple way to boost nature connectedness: boost and engage people with visible biodiversity.},
	number = {2},
	urldate = {2023-10-12},
	journal = {Ecopsychology},
	author = {Hamlin, Iain and Richardson, Miles},
	month = jun,
	year = {2022},
	note = {Publisher: Mary Ann Liebert, Inc., publishers},
	keywords = {Nature connectedness, Garden biodiversity, Noticing nature, Pronature conservation behavior},
	pages = {111--117},
	file = {Full Text PDF:/home/drogo/Zotero/storage/3J8YC44A/Hamlin and Richardson - 2022 - Visible Garden Biodiversity Is Associated with Not.pdf:application/pdf},
}

@article{kaiser_contrasting_2005,
	title = {Contrasting the {Theory} of {Planned} {Behavior} {With} the {Value}-{Belief}-{Norm} {Model} in {Explaining} {Conservation} {Behavior}},
