@article{ WOS:000855990300001,
Author = {Prendergast, Kit S. and Ollerton, Jeff},
Title = {Spatial and temporal scale of analysis alter conclusions about the
   effects of urbanisation on plant-pollinator networks},
Journal = {ARTHROPOD-PLANT INTERACTIONS},
Year = {2022},
Volume = {16},
Number = {6},
Pages = {553-565},
Month = {DEC},
Abstract = {Bipartite networks of flowering plants and their visitors (potential
   pollinators) are increasingly being used in studies of the structure and
   function of these ecological interactions. Whilst they hold much promise
   in understanding the ecology of plant-pollinator networks and how this
   may be altered by environmental perturbations, like land-use change and
   invasive species, there is no consensus about the scale at which such
   networks should be constructed and analysed. Ecologists, however, have
   emphasised that many processes are scale dependent. Here, we compare
   network- and species-level properties of ecological networks analysed at
   the level of a site, pooling across sites within a given habitat for
   each month of surveys, and pooling across all sites and months to create
   a single network per habitat type. We additionally considered how these
   three scales of resolution influenced conclusions regarding differences
   between networks according to two contrasting habitat types (urban
   bushland remnants and residential gardens) and the influence of honey
   bee abundance on network properties. We found that most network
   properties varied markedly depending on the scale of analysis, as did
   the significance, or lack thereof, of habitat type and honey bee
   abundance on network properties. We caution against pooling across sites
   and months as this can create unrealistic links, invalidating
   conclusions on network structure. In conclusion, consideration of scale
   of analysis is also important when conducting and interpreting
   plant-pollinator networks.},
DOI = {10.1007/s11829-022-09925-w},
EarlyAccessDate = {SEP 2022},
ISSN = {1872-8855},
EISSN = {1872-8847},
ResearcherID-Numbers = {Ollerton, Jeff/AAE-3946-2020
   Prendergast, Kit/T-3192-2019
   },
ORCID-Numbers = {Prendergast, Kit/0000-0002-1164-6099
   Ollerton, Jeff/0000-0002-0887-8235},
Unique-ID = {WOS:000855990300001},
