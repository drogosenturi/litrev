@article{ WOS:000485270700001,
Author = {Whitburn, Julie and Linklater, Wayne L. and Milfont, Taciano L.},
Title = {Exposure to Urban Nature and Tree Planting Are Related to
   Pro-Environmental Behavior via Connection to Nature, the Use of Nature
   for Psychological Restoration, and Environmental Attitudes},
Journal = {ENVIRONMENT AND BEHAVIOR},
Year = {2019},
Volume = {51},
Number = {7},
Pages = {787-810},
Month = {AUG},
Abstract = {Pro-environmental behaviors (PEBs) may be associated with a personal
   relationship with nature. We conducted a quasi-experiment with 423
   residents who had or had not actively participated in a tree-planting
   scheme and lived in 20 neighborhoods that differ in their greenness
   level. We tested whether exposure to nature (specified by the amount of
   vegetation cover in residents' neighborhoods) and/or past PEB (specified
   by tree-planting participation) was associated with residents' PEB and
   whether key psychological constructs would mediate these relationships.
   Structural equation mediation models revealed that the level of
   neighborhood vegetation and involvement in the planting scheme explained
   46\% of the variance in PEB, with connection to nature, the use of
   nature for psychological restoration, and environmental attitudes
   mediating the relationships. Connection to nature was more strongly
   associated with engagement in PEB than the use of nature for
   psychological restoration and environmental attitudes.},
DOI = {10.1177/0013916517751009},
ISSN = {0013-9165},
