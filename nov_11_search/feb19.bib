@article{ WOS:000637272700001,
Author = {Ahmadi, Esmaeil and Roumi, Soheil and Aligholian, Armin and Aslani,
   Alireza},
Title = {Analysis of optimal energy supply in the commercial buildings: a bird
   garden case study},
Journal = {INTERNATIONAL JOURNAL OF AMBIENT ENERGY},
Year = {2021},
Month = {2021 APR 3},
Abstract = {Energy consumption in residential and commercial buildings is one of the
   main scopes of energy studies. These buildings have a high potential for
   saving energy and applying renewable energy technologies. This research
   goal is to explore an energy demand analysis in a commercial birds'
   exhibition building. In the first step, the heating and cooling loads
   are calculated with consideration of the birds' thermal needs. In the
   next step, total electricity demand is determined by visual inspection,
   and a new approach is proposed for saving energy by classifying the
   birds in the studied building. Finally, an optimal combination of
   electricity generation resources, including wind and solar energy
   resources, is designed. The result is compared with the current
   electricity supply (electricity grid) based on economic and
   environmental factors. The results indicate that energy consumption is
   decreased by about 21.49\% based on the introduced approach, while the
   renewable strategy is economically reasonable. This energy-saving caused
   31\% lower NPV for the grid-connected energy supply and 21\% lower NPV
   for the renewable-powered energy supply.},
DOI = {10.1080/01430750.2021.1907617},
