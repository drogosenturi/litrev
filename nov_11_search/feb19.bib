   Fonseca, Carlos/D-9744-2011},
ORCID-Numbers = {Rosalino, Luís Miguel/0000-0003-4186-7332
   Lino, Sofia/0000-0002-6428-4845
   Fonseca, Carlos/0000-0001-6559-7133},
Unique-ID = {WOS:001285687800001},
